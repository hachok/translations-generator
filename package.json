{
  "name": "translations-generator",
  "version": "1.3.3",
  "description": "translations-generator generates translations with label and text for json and js files.",
  "bin": {
    "translations-generator": "index.js"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "test": "echo \"No test specified\"",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prettier": "prettier --write \"{,!(node_modules)/**/}*.{js,ts,json,md}\"",
    "pre-commit": "npm run lint:fix && npm run prettier",
    "clean": "shx rm -rf index.js",
    "build": "npm run clean && webpack"
  },
  "repository": "git@github.com:hachok/translations-generator.git",
  "keywords": [
    "frontend",
    "backend",
    "translations",
    "generate",
    "generator",
    "create",
    "translation",
    "label",
    "text",
    "add"
  ],
  "author": "hachok",
  "license": "MIT",
  "dependencies": {
    "chalk": "2.4.2",
    "readline-sync": "1.4.10"
  },
  "devDependencies": {
    "@types/node": "13.13.4",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "shx": "0.3.2",
    "ts-loader": "7.0.2",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "npm run pre-commit",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
